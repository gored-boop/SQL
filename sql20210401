--*은 모든 열 이라는 의미로 사용된다.
SELECT * --줄 끝까지 주석으로 처리됩니다.
FROM employees; --명령어는 대문자로 사용을 권장(필수는 아님)

SELECT employee_id, first_name, last_name
FROM employees; 


SELECT employee_id, first_name, last_name
FROM employees --명령어는 대문자로 사용을 권장(필수는 아님)

ORDER BY employee_id ASC; --DESC 내림차순

--특정열만 출력

SELECT job_id
FROM employees;

--별명 사용 AS...

SELECT employee_id AS 사원번호, first_name AS 이름, last_name AS 성
FROM employees;
